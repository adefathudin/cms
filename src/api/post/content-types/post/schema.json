{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "post"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "content": {
      "type": "richtext"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "ShortPost",
        "LongPost",
        "Stories",
        "Article",
        "URL",
        "Document",
        "ShortPhoto",
        "WithAI"
      ]
    },
    "sticky": {
      "type": "boolean",
      "default": false
    },
    "active": {
      "type": "boolean",
      "default": true
    },
    "source": {
      "type": "string"
    },
    "tag": {
      "type": "string"
    },
    "media": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    }
  }
}
